generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model tour_types {
  id              Int     @id @default(autoincrement())
  parent          Int?
  name            String?
  type            String?
  url             String?
  photo           String
  description     String
  title           String?
  metakeywords    String
  metadescription String
  sorting         Int
}
